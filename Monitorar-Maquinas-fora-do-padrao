config case_sensitive = false |
dataset = endpoints
| filter endpoint_name !~= "brmo|BRMO|VDIWIN|NDEL|DDEL|NLEN|DLEN|NHP|DHP|VDI|DBRG|FC"
| filter operating_system !~= "Server"
| filter installation_package !~= "Linux"
| alter install_date = timestamp_diff(current_time(), install_date, "DAY")
| filter install_date = 30
| alter last_seen  = timestamp_diff(current_time(), last_seen, "DAY")
| filter last_seen <= 7
| filter last_seen != install_date  
| fields endpoint_name, user ,ip_address, operating_system, last_seen, install_date 
